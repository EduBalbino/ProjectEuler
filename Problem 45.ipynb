    "trig_numb = set()\n",
    "pent_numb = set()\n",
    "hexag_numb = set()\n",
    "i = 1\n",
    "while i < 100000:\n",
    "    i += 1\n",
    "    t = (i*i + i) // 2\n",
    "    p = (3*i*i - i) // 2\n",
    "    h = (2*i*i - i)\n",
    "    if t in pent_numb and t in hexag_numb:\n",
    "        print(\"Solution =\", t)\n",
    "    pent_numb.add(p)\n",
    "    hexag_numb.add(h)"
   ]
